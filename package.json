{
  "name": "axe-crawler",
  "version": "0.1.3",
  "description": "A website crawler meant to automatically test a website for accessibility issues using the axe-core library",
  "main": "dist/index.js",
  "repository" : {
    "type": "git",
    "url" : "https://github.com/tjscollins/axe-crawler"
  },
  "scripts": {
    "build": "NODE_ENV=production node_modules/rollup/bin/rollup -c rollup.config.js",
    "test": "jest"
  },
  "bin": {
    "axe-crawler": "bin/axe-crawler"
  },
  "author": "Tyler Collins",
  "license": "MIT",
  "dependencies": {
    "axe-webdriverjs": "^1.1.5",
    "axios": "^0.16.2",
    "cheerio": "^1.0.0-rc.2",
    "escape-html": "^1.0.3",
    "marked": "^0.3.6",
    "minimist": "^1.2.0",
    "selenium-webdriver": "^3.5.0",
    "validator": "^8.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "eslint": "^4.7.2",
    "eslint-config-airbnb-base": "^12.0.1",
    "eslint-plugin-import": "^2.7.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-sourcemaps": "^2.6.1",
    "jest": "^21.2.1",
    "mocha": "^3.5.3",
    "moxios": "^0.4.0",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "sinon": "^4.0.0",
    "webpack": "^3.6.0",
    "webpack-stream": "^3.2.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/"
    ]
  }
}
